{
  "name": "kookee",
  "version": "1.0.0",
  "scripts": {
    "start": "nodemon src/server.js",
    "start:prod": "NODE_ENV=production nodemon src/server.js",
    "watch:sass": "node-sass src/assets/sass/main.scss src/assets/css/style.css -w",
    "live-server": "live-server --open=public",
    "build:js": "parcel watch src/assets/js/index.js --dist-dir src/assets/js/dist",
    "build": "npm-run-all -p watch:sass build:js"
  },
  "author": "khanh",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.23.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "nodemailer": "^6.7.0",
    "pug": "^3.0.2",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "live-server": "^1.2.1",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0"
  }
}
